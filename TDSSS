loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local colors = {
    SchemeColor = Color3.fromRGB(75, 99, 135),
    Background = Color3.fromRGB(30, 32, 38),
    Header = Color3.fromRGB(24, 25, 29),
    TextColor = Color3.fromRGB(255,255,255),
    ElementColor = Color3.fromRGB(31, 30, 37)
}
local Window = Library.CreateLib("Flare Hub", colors)
local Home = Window:NewTab("Home")
local Home = Home:NewSection("Home")
local Towers = Window:NewTab("Towers")
local Towers = Towers:NewSection("Towers")
local AutoSkip = Window:NewTab("AutoSkip")
local AutoSkip = AutoSkip:NewSection("AutoSkip")
local S = Window:NewTab("Settings")
local S = S:NewSection("UI")
S:NewKeybind("Keybind", "", Enum.KeyCode.F, function()
	Library:ToggleUI()
end)
AutoSkip:NewToggle("AutoSkip", "ToggleInfo", function(state)
    if state then
     _G.Toggle = true

     while _G.Toggle do
     wait(1)
     game.ReplicatedStorage.RemoteFunction:InvokeServer("Waves", "Skip")
    end

    else
     _G.Toggle = false

     while _G.Toggle do
     wait(1)
     game.ReplicatedStorage.RemoteFunction:InvokeServer("Waves", "Skip")
    end
    end
end)
Home:NewButton("Made By Testerextra", "you are pro", function()
    print("pro")
end)
Home:NewButton("Private Server", "Click To Copy Link", function()
    setclipboard("https://web.roblox.com/games/3260590327?privateServerLinkCode=13206073949552273032140132135759")
end)
Towers:NewButton("Place Double", "you are pro", function()
    print("pro")
end)
Towers:NewTextBox("Towers", "Equip", function(txt)
	-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Inventory",
    [2] = "Execute",
    [3] = "Troops",
    [4] = "Add",
    [5] = {
        ["Name"] = (txt)
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

end)
Towers:NewTextBox("RemoveTowers", "Remove", function(txt)
	-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Inventory",
    [2] = "Execute",
    [3] = "Troops",
    [4] = "Remove",
    [5] = {
        ["Name"] = (txt)
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

end)
